<?xml version="1.0" encoding="UTF-8"?>
<AppConfig>

    <!-- 向右解锁距离，相对于 720p, 默认400 -->

    <item
        name="unlockDis"
        value="400"/>
    <!-- 向右解锁距离，相对于 720p, 相对于屏幕底边缘，默认700 -->

    <item
        name="unlockY"
        value="0"/>
    <!-- 向右解锁距离，相对于 720p,相对于屏幕底边缘, 默认0 -->

    <item
        name="lephoneY"
        value="0"/>
    <!-- 锁屏背景路径 1 动态换壁纸路径，最优先读取的壁纸路径 默认/data/data/com.iLoong.base.themebox/lockwallpapers/lock.png -->

    <item
        name="backgroundPath1"
        value="/data/data/com.iLoong.base.themebox/lockwallpapers/lock.png"/>

    <!-- 锁屏背景路径 2 作为默认壁纸路径，次读取的路径，一般为恢复出厂设置不可删掉的路径，无则读取apk自身壁纸 -->

    <item
        name="backgroundPath2"
        value="system/lock.png"/>

</AppConfig>